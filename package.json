{
  "name": "recollect",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "cdn": "rsync -avr --progress dist/* tiye.me:cdn/recollect",
    "tiye": "rsync -avr --progress dist/{index.html,manifest.json} tiye.me:repo/mvc-works/recollect/",
    "upload": "yarn cdn && yarn tiye",
    "release": "shadow-cljs release browser",
    "watch": "shadow-cljs watch browser",
    "watch-test": "shadow-cljs watch test",
    "compile-ssr": "shadow-cljs compile ssr",
    "compile-test": "shadow-cljs compile test",
    "html-dev": "env=dev node target/ssr.js",
    "html": "node target/ssr.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn compile-ssr && yarn html-dev && yarn dev-manifest && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist",
    "dev-manifest": "cp entry/manifest.json target/",
    "manifest": "cp entry/manifest.json dist/",
    "test": "yarn compile-test && node target/test.js",
    "build": "yarn del && yarn release && yarn webpack && yarn compile-ssr && yarn html && yarn manifest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Cumulo/recollect.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.12.7",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "http-server": "^0.10.0",
    "shadow-cljs": "^2.0.52",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "^1.1.2"
  }
}
